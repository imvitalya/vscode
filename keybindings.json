// Place your key bindings in this file to override the defaults
[
  {
    "key": "shift+cmd+g",
    "command": "workbench.view.scm",
    "when": "workbench.scm.active"
  },
  {
    "key": "ctrl+shift+g",
    "command": "-workbench.view.scm",
    "when": "workbench.scm.active"
  },
  {
    "key": "cmd+n", // "cmd+n" on mac
    "command": "extension.advancedNewFile",
  },
  {
    "key": "ctrl+shift+s",
    "command": "workbench.action.files.saveAs"
  },
  {
    "key": "shift+cmd+s",
    "command": "-workbench.action.files.saveAs"
  },
  {
    "key": "cmd+k s",
    "command": "-workbench.action.files.saveWithoutFormatting"
  },
  {
    "key": "cmd+s",
    "command": "workbench.action.files.save"
  },
  {
    "key": "cmd+s",
    "command": "-workbench.action.files.save"
  },
  {
    "key": "shift+cmd+s",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "cmd+b",
    "command": "-workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "shift+cmd+t",
    "command": "git.pullRef",
    "when": "scmProvider == 'git'"
  },
  {
    "key": "shift+cmd+t",
    "command": "-workbench.action.reopenClosedEditor"
  },
  {
    "key": "shift+cmd+y",
    "command": "workbench.view.extension.yandex-music-explorer",
    "when": "viewContainer.workbench.view.extension.yandex-music-explorer.enabled"
  },
  {
    "key": "shift+alt+y shift+alt+m",
    "command": "-workbench.view.extension.yandex-music-explorer",
    "when": "viewContainer.workbench.view.extension.yandex-music-explorer.enabled"
  },
  {
    "key": "ctrl+shift+l",
    "command": "workbench.action.toggleLightDarkThemes"
  },
  {
    "key": "ctrl+tab",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "ctrl+cmd+i",
    "command": "-workbench.action.chat.open",
    "when": "!chatSetupHidden"
  },
  {
    "key": "ctrl+cmd+i",
    "command": "-workbench.panel.chat",
    "when": "workbench.panel.chat.view.copilot.active"
  },
  {
    "key": "shift+cmd+t",
    "command": "workbench.action.toggleAuxiliaryBar"
  },
  {
    "key": "alt+cmd+b",
    "command": "-workbench.action.toggleAuxiliaryBar"
  }
]